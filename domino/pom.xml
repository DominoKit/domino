<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.progressoft.brix.domino</groupId>
    <artifactId>domino</artifactId>
    <version>1.0-rc.2-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>domino</name>


    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    </properties>

    <url>https://github.com/GwtDomino/domino</url>
    <description>GWT Domino</description>

    <licenses>
        <license>
            <name>Apache 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0</url>
        </license>
    </licenses>

    <developers>
        <developer>
            <name>Ahmad K. Bawaneh</name>
            <email>akabme@gmail.com</email>
        </developer>
        <developer>
            <name>Rafat J. Al-Barouki</name>
            <email>rafat.albarouki@gmail.com</email>
        </developer>
        <developer>
            <name>Farah M. Soudani</name>
            <email>fsoudanii93@gmail.com</email>
        </developer>
    </developers>

    <issueManagement>
        <url>https://github.com/GwtDomino/domino/issues</url>
        <system>GitHub Issues</system>
    </issueManagement>

    <scm>
        <connection>scm:git:https://github.com/GwtDomino/domino.git</connection>
        <developerConnection>scm:git:git@github.com:GwtDomino/domino.git</developerConnection>
        <url>https://github.com/GwtDomino/domino</url>
    </scm>

    <distributionManagement>
        <snapshotRepository>
            <id>ossrh</id>
            <url>https://oss.sonatype.org/content/repositories/snapshots</url>
        </snapshotRepository>
    </distributionManagement>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.progressoft.brix.domino.apt</groupId>
                <artifactId>apt-client</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.apt</groupId>
                <artifactId>apt-server</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.api</groupId>
                <artifactId>domino-api-client</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.api</groupId>
                <artifactId>domino-api-client</artifactId>
                <version>${project.version}</version>
                <classifier>sources</classifier>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.api</groupId>
                <artifactId>domino-api-server</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.api</groupId>
                <artifactId>domino-api-shared</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.api</groupId>
                <artifactId>domino-api-shared</artifactId>
                <version>${project.version}</version>
                <classifier>sources</classifier>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino</groupId>
                <artifactId>domino-client-commons</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.logging</groupId>
                <artifactId>domino-gwt-logger</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.logging</groupId>
                <artifactId>domino-log4j-server-logging</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino</groupId>
                <artifactId>domino-remote-logging</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.test</groupId>
                <artifactId>domino-client-test</artifactId>
                <version>${project.version}</version>
                <scope>test</scope>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino</groupId>
                <artifactId>jks-server-cofigurator</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino</groupId>
                <artifactId>domino-service-discovery</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.test</groupId>
                <artifactId>domino-server-test</artifactId>
                <version>${project.version}</version>
                <scope>test</scope>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.impl</groupId>
                <artifactId>domino-gwt</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino.impl</groupId>
                <artifactId>domino-gwt</artifactId>
                <version>${project.version}</version>
                <classifier>sources</classifier>
            </dependency>
            <dependency>
                <groupId>com.progressoft.brix.domino</groupId>
                <artifactId>domino-desktop</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>javax.servlet</groupId>
                <artifactId>javax.servlet-api</artifactId>
                <version>3.1.0</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
</project>